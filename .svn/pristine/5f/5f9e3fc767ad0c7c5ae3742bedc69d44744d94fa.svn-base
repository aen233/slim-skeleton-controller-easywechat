namespace App\Library;

use App\Event\EventReceiver;

/**
 * Class Core
 * @package App\Library
 */
class Core
{
    protected $event;

    /**
     * Core constructor.
     * @param EventReceiver $event
     */
    public function __construct(EventReceiver $event)
    {
        $this->event = $event;

    }

    /**
     * @param $message
     */
    public function MsgHandle($message)
    {
        $this->event->handle($message);
    }
}